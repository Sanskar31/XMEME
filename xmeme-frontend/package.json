{
  "name": "xmeme-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dotenv": "^0.1.3",
    "react-loader-spinner": "^4.0.0",
    "react-responsive-modal": "^6.0.1",
    "react-scripts": "4.0.2",
    "react-smooth-image": "^1.1.0",
    "react-spring": "^9.0.0-rc.3",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-dotenv && npm run watch:css && react-scripts start",
    "build": "react-dotenv && npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  },
  "react-dotenv": {
    "whitelist": [
      "BACKEND_URL"
    ]
  }
}
